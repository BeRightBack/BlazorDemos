{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.3.2",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/bootstrap-icons/"
    },
    {
      "library": "Chart.js@4.4.1",
      "destination": "wwwroot/chartjs/"
    },
    {
      "library": "chartjs-plugin-datalabels@2.2.0",
      "destination": "wwwroot/chartjs-plugin-datalabels/"
    }
  ]
}